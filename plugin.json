{
	"logo": "logo.png",
	"main": "index.html",
	"preload": "preload.js",
	"name": "batchReader",
	"version": "0.0.2",
	"pluginName": "票据小助手",
	"description": "识别PDF电子票据，并提取内容",
	"author": "empyrealtear",
	"homepage": "https://github.com/empyrealtear",
	"features": [
		{
			"code": "main",
			"explain": "读取票据",
			"cmds": [
				"reader",
				"rd"
			]
		},
		{
			"code": "ReaderFiles",
			"explain": "读取票据",
			"cmds": [
				{
					"type": "files",
					"label": "读取票据",
					"fileType": [
						"file",
						"directory"
					],
					"match": "/\\.(pdf|ofd)$/i",
					"minLength": 1
				}
			]
		}
	]
}